	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start --resetCache",
		"packager": "./node_modules/react-native/packager/packager.sh start --resetCache",
		"android-bundle": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
		"android-build": "cd android && ./gradlew assembleRelease",
		"android-install": "cd android && ./gradlew installRelease",
		"release": "yarn android-bundle && yarn android-build && yarn android-install",
		"lint": "./node_modules/.bin/eslint .",
		"test": "jest"
	},
